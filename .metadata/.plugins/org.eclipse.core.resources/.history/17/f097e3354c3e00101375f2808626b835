package CountNumberOfDigit;

import java.util.Scanner;

public class ClassA {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int num = s.nextInt();
		
		int count =num;
		
		
//		int num=2546380;
//		int count=0;
		
		while (num>0) {
			num=num/10;
			count++;
			System.out.println("Enter a Numbers");
		}
		System.out.println("Number of digits "+count);
	}

}
