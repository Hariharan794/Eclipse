package ReverseStr;

public class ClasssA {
	public static void main(String[] args) {
		String s ="programming";
		
//		for (int i =s.length()-1; i >=0; i--) {
//			char charAt = s.charAt(i);
//			System.out.print(charAt);
//		}
		String rev ="";
		
		for (int i = s.length()-1; i >=0; i--) {
			char charAt = s.charAt(i);
			rev=rev+charAt;
			
			System.out.println(s);
			System.out.println(rev);
		
		
	}

}
